node_modules/

# Ignore build and dist folders
build/
dist/

# Ignore environment files
.env
.env.local

# Ignore lock files
package-lock.json
yarn.lock

# Ignore JSON data and logs
*.log
*.json

# Ignore public assets (images, videos, etc.)
public/

# Ignore Prettier config itself
.prettierrc
